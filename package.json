{
  "name": "@amazingcat/amazing-iohook",
  "version": "8.1.1",
  "description": "Simplified fork of iohook â€” global keyboard and mouse listener",
  "main": "index.js",
  "scripts": {
    "test": "xo",
    "install": "node install.js",
    "example-counter": "node ./examples/counter.js",
    "electron-example-counter": "electron ./examples/counter.js"
  },
  "repository": "https://github.com/cattr-app/amazing-iohook",
  "preloadBinaries": {
    "repo": "https://repo.amazingcat.net/node/amazing-iohook/builds",
    "branch": "latest",
    "arch": [
      "linux64",
      "darwin64",
      "win3264"
    ],
    "runtime": {
      "type": "electron",
      "version": "8.1.1"
    },
    "signingKey": ""
  },
  "author": "Cattr Team & Contributors <hi@cattr.app>",
  "license": "MIT",
  "xo": {
    "space": 2,
    "rules": {
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 2,
          "maxEOF": 1
        }
      ],
      "max-statements-per-line": [
        "error",
        {
          "max": 2
        }
      ],
      "padded-blocks": [
        "error",
        "always"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "curly": [
        "error",
        "multi"
      ],
      "no-return-assign": [
        "error",
        "except-parens"
      ]
    }
  },
  "devDependencies": {
    "xo": "^0.25.3"
  },
  "optionalDependencies": {
    "electron": "7.1.4"
  }
}
